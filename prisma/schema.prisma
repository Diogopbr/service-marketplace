generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Service {
  id             String   @id @default(uuid())
  title          String
  slug           String   @unique
  description    String
  category       String   @default("outros")
  price          Float
  favoritesCount Int      @default(0)
  createdAt      DateTime @default(now())
}
